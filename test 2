<html>
	<head>
		<title>Mijn site</title>
		<script type="text/javascript">

		//zorgt voor de input
		var scoreteam1 = prompt("Wat is de score van team 1?");
		var scoreteam2 = prompt("Wat is de score van team 2?");

		if (scoreteam1 > scoreteam2) {
			alert("Team 1 heeft gewonnen")
		} else{ 
			alert("Team 1 heeft gewonnen")
		}

		if (scoreteam1 == scoreteam2) {
			alert(" Het is gelijkspel")
		}

		</script>
	</head>
	<body>
		<h1>Hallo</h1>
	</body>
</html>